			$("#media_reg_info_modal").on("hidden.bs.modal", function(e) {
				if (showTip) {
					$("#perfect_principal_info_modal").modal("show");
				} else {
					showTip = true;
				}
			});

			$("#media_reg_info_modal").on("change", "[name=mediaType]", function() {
				var that = this;
				var mediaType = $(that).val();
				var text = '<label class="required_label">*</label>';

				var principalDescDom = $("[for=principalDescription]").parent(".form-group");

				if (mediaType == 1) {
					text += "负责人名字";
					principalDescDom.hide();
				} else {
					text += "公司/工作室名称";
					principalDescDom.show();
				}
				$("#media_reg_info_modal [for=principalName]").html(text);
			});

			$("#media_reg_info_modal").on("change", "[name=payMethod]", function() {
				var that = this;
				var type = $(that).val();
				if (type == 1) {
					$("#vinfo input[name]").removeAttr("readonly");
					$("#bankInfo input[name]").prop("readonly", "readonly");
					$("#bankInfo select[name]").prop("disabled", "disabled");

				} else {
					$("#vinfo input[name]").prop("readonly", "readonly");
					$("#bankInfo input[name]").removeAttr("readonly");
					$("#bankInfo select[name]").removeAttr("disabled");
				}
			});

			$("#media_reg_info_modal").on("shown.bs.modal", function(e) {

				$.post("/daren-web/daren/getCurrentUserRegInfo", {}, function(resp) {
					var html = template("principal_inf_edit_temp", resp.result);
					$("#principal_info_tabcontent").html(html);

					$.post("/daren-web/daren/loadCMSOptionLog", {
						"entityType": "t_dren_principal",
						"entityId": resp.result.principalId
					}, function(d) {


						var principalEntityDesc = {
							"principalName": "主体名称",
							"mediaType": "帐号类型",
							"principalDescription": "主体简介",
							"contacts": "业务联系人",
							"contactTitle": "职位",
							"contactQq": "QQ号",
							"contactDingding": "钉钉号",
							"contactPhone": "联系电话",
							"contactWechat": "微信号",
							"contactWangwang": "旺旺号",
							"paymentSettle_method": "结算方式",
							"paymentInvoice_type": "开票类型",
							"paymentPayeName": "收款人姓名",
							"paymentPayeeBank": "开户银行",
							"paymentPayeeBank_account": "收款人银行卡号",
							"paymentPayeeBank_branch": "开户支行",
							"paymentPayeePhone": "收款人电话",
							"paymentPayeeId_card": "收款人身份证号",
							"paymentPayeeVAccount": "V账号"
						};

						$.each(d.result, function(idx, item) {
							var content = JSON.parse(item.logContent);
							var contentStr = "";
							$.each(content, function(idx2, item2) {
								var fieldStr = principalEntityDesc[item2.field];
								if (isEmpty(fieldStr))
									return true;
								contentStr += "[" + fieldStr + "]&nbsp;&nbsp;&nbsp;由：“" + item2.from + "”&nbsp;&nbsp;&nbsp;改为：“" + item2.to + "”；<br>";
							});
							$.extend(item, {
								"logContentStr": contentStr
							});
						});

						var logsHtml = template("principal_change_logs", d);
						$("#collapsePrincipalChangeLogsContent").html(logsHtml);
					});

					var selectOption = {
						dropdownParent: $("#media_reg_info_modal")
					}
					setDic("add_dr_payee_bank", dict.BANK, resp.result.paymentPayeeBank, selectOption);
					setDic("add_or_edit_invoice_type", dict.SETTLEMENT_INVOICE_TYPE, resp.result.paymentInvoiceType, selectOption);
					setDic("add_or_edit_settlement_period", dict.SETTLEMENT_PERIOD, resp.result.paymentSettlePeriod, selectOption);

					var settleMethods = dict.SETTLEMENT_METHOD;
					delete settleMethods[1];
					setDic("add_or_edit_settlement_method", dict.SETTLEMENT_METHOD, resp.result.paymentSettleMethod, selectOption);
					$("#add_dr_payee_bank").prepend("<option value='0'>请选择</option>");

					if (resp.result.paymentSettleMethod == 1) {
						$("[name=payMethod][value=1]").click();
					} else {
						$("[name=payMethod][value=0]").click();
					}

					var tabName = isNotEmpty(window.principalDefaultTab) ? window.principalDefaultTab : 'regInfo_basic';
					var tabBtn = $("#media_reg_info_modal [aria-controls=" + tabName + "]");
					var tabBtn1 = $("#media_reg_info_modal [aria-controls=regInfo_basic]");
					//点击tab
					tabBtn1[0].click();
					tabBtn[0].click();
				});
			});

			//保存主体信息
			$("#media_reg_info_modal").on("click", "[bind-method=submitMediaRegInfo]", function(ev) {
				var that = this;
				var data = {};
				var formName = $(that).attr("validate-form");
				var formDataArray = $("#" + formName).serializeArray();
				$.each(formDataArray, function() {
					data[this.name] = this.value;
				});
				submitMediaRegInfo(that, data);
			});

			//添加达人
			$("#add_daren_info_modal").on("shown.bs.modal", function(e) {
				var that = this;
				$(that).find("form")[0].reset();
				var selectOption = {
					dropdownParent: $("#add_daren_info_modal")
				}
				setDic("add_daren_info_platform", dict.WE_MEDIA_PLATFORM, selectOption);
				setDic("add_domain", dict.DR_DOMAIN, selectOption);
				$("#add_daren_info_platform").prepend("<option value='0' >请选择</option>");
			});
			$("#add_daren_info_modal").on("hidden.bs.modal", function(e) {
				$("#perfect_principal_info_modal").modal("show");
			});

			//添加达人信息
			$("#add_daren_info_modal").on("click", "[bind-method=submitDarenInfo]", function(ev) {
				var that = this;
				var data = {};
				var formName = $(that).attr("validate-form");
				var formDataArray = $("#" + formName).serializeArray();
				$.each(formDataArray, function() {
					data[this.name] = this.value;
				});

				if (data.platform == 0) {
					alert("平台不能为空");
					return;
				}
				if (isEmpty(data.nickname)) {
					alert("昵称不能为空");
					return;
				}

				if (data.platform == 1) {
					if (data.resourceType == 0) {
						alert("已发布链接不能为空");
						return;
					}
					if (isEmpty(data.articleUrl)) {
						alert("已发布链接不能为空");
						return;
					}
				}

				$.post("/daren-web/daren/addDaren", data, function(resp) {
					if (resp.status == 0) {
						alert("新增达人成功，继续添加渠道报价");
						$("#add_daren_info_modal").modal("hide");
						$.extend(resp.result, {
							platformStr: dict.WE_MEDIA_PLATFORM[resp.result.platform],
							articleChannelStr: dict.RESOURCE_LOCATION[resp.result.resourceType]
						});
						openDarenResourceManagerModal(resp.result);
						return;
					}
					alert(resp.msg);
				});
			});

		});

		function signOut() {
			try {
				window.localStorage.removeItem("accountinfo");

			} catch (e) {
				console.log(e);
			}

			$.ajax({
				url: "/daren-web/rbac/logout.wb",
				success: function() {
					var loginUrl = "/login.html";
					parent.location.href = loginUrl;
				}
			})
		}

		function mizhu_close() {
			$('.plug-step-win').fadeOut("fast");
		}

		function reinstall_close() {
			$('.plug-reinstall-win').fadeOut("fast");
		}

		function showEditProfile() {
			$("#edit_profile").modal("show");
			$.ajax({
				url: "/daren-web/rbac/user/profile.wb",
				success: function(rest) {
					if (rest.status != 0) {
						alert(rest.msg);
					} else {
						$("#nickname").val(rest.result.nick);
						$("#email").val(rest.result.email);
					}
				}
			})

		}

		function submitEditProfile() {
			var nickname = $("#nickname").val();
			var email = $("#email").val();
			if (!email || !nickname) {
				alert("姓名和Email都必填！");
				return false;
			}
			$.ajax({
				url: "/daren-web/rbac/user/editProfile.wb",
				type: "post",
				data: {
					nick: nickname,
					email: email
				},
				success: function(rest) {
					if (rest.status != 0) {
						alert(rest.msg);
					} else {
						alert("修改成功！");
						$("#edit_profile").modal("hide");
					}
				}
			})
		}

		function submitChangePassword() {
			var oldPassword = $.trim($("#oldPassword").val());
			var newPassword = $.trim($("#newPassword").val());
			var confirmPassword = $.trim($("#confirmPassword").val());
			if (!oldPassword || !newPassword || !confirmPassword) {
				alert("所有输入不能为空！");
				return false;
			}
			if (oldPassword.length < 6) {
				alert("旧密码不正确！");
				$("#oldPassword").focus();
				return false;
			}
			if (newPassword.length < 6) {
				alert("密码长度不小于6位！");
				$("#newPassword").focus();
				return false;
			}

			var regx = /^(?!([a-zA-Z]+|\d+)$)[a-zA-Z\d]{6,}$/;
			if (newPassword.match(regx) == null) {
				alert("密码必须同时包含字母和数字！");
				$("#newPassword").focus();
				return false;
			}

			if (newPassword != confirmPassword) {
				alert("两次输入密码不相同！");
				$("#confirmPassword").val();
				$("#confirmPassword").focus();
				return false;
			}
			$.ajax({
				url: "/daren-web/rbac/user/changePassword.wb",
				data: {
					oldPassword: oldPassword,
					newPassword: newPassword
				},
				success: function(rest) {
					if (rest.status != 0) {
						alert(rest.msg);
					} else {
						alert("修改密码成功，重新登录！");
						var loginUrl = "/login.html";
						parent.location.href = loginUrl;
					}
				}
			})
		}

		function showChangePassword() {
			$("#change_password").modal("show");
		}

		function smartSetHandle(target) {
			var eventBySmartSet = document.createEvent('Event');
			eventBySmartSet.initEvent('eventBySmartSet', true, true);
			$(target)[0].dispatchEvent(eventBySmartSet);
		}

		function rollBackDataHandle(target) {
			var eventByRollBackData = document.createEvent('Event');
			eventByRollBackData.initEvent('eventByRollBackData', true, true);
			$(target)[0].dispatchEvent(eventByRollBackData);
		}

		function initMenu() {
			layerConfig();
			contabsInit();
		}
		
	</script>
	
