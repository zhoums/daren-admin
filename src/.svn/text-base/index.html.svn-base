		function initPerfectPrincipalInfoModalData() {
			$.post("/daren-web/daren/mediaPrincipalInfoIsPerfect", {}, function(data, textStatus, req) {

				$("#moliContactEmail").text("如有疑问，可联系&nbsp;" + data.result.moliContactEmail);

				$("#perfect_principal_info_modal #audit_failure_msg").html(data.result.principalInfo.auditRemark);
				var html = template("perfect_principal_info_temp", data.result);
				$("#perfect_info_fieldset").html(html);

				var pending_review_tab = template("pending_review_tab_fieldset_temp", data.result);
				$("#pending_review_tab").html(pending_review_tab);

				var audit_failure_tab = template("audit_failure_tab_fieldset_temp", data.result);
				$("#audit_failure_tab_body").html(audit_failure_tab);

				var more_info_tab = template("more_info_tab_fieldset_temp", data.result);
				$("#more_info_tab_body").html(more_info_tab);

				//当前主体状态
				var principalStatus = data.result.principalInfo.auditStatus;

				(function() {
					//是否可以提交审核
					var isPerfects = [data.result.drenResInfo.isPerfect, data.result.drenInfo.isPerfect, data.result.paymentInfo.isPerfect, data.result.contactInfo.isPerfect,
						data.result.basicPrincipalInfo.isPerfect
					];
					var canAuditSubmit = true;
					$.each(isPerfects, function() {
						if (this == 2) {
							canAuditSubmit = false;
							return false;
						}
					});

					var statusCanSubmit = false;
					var canSubmitAuditStatus = [0, 1, 12, 22, 32, 99];
					//是否可以提交审核
					if (canSubmitAuditStatus.contains(principalStatus)) {
						statusCanSubmit = true;
					}

					if (canAuditSubmit && statusCanSubmit) {
						$(".audit_submit_btn").attr("cansubmit", 1);
					} else {
						$(".audit_submit_btn").attr("cansubmit", 0);
					}
				})();

				//控制是否可关闭tip
				(function() {
					var canCloseTipStatus = [11, 21, 31, 90];
					//审核不通过
					if (canCloseTipStatus.contains(principalStatus)) {
						$("#close_perfect_principal_info_modal_btn").attr("canclose", 1);
					} else {
						$("#close_perfect_principal_info_modal_btn").attr("canclose", 0);
					}
				})();

				(function() {
					var auditFailureStatus = [12, 22, 32, 99];
					if (auditFailureStatus.contains(principalStatus)) {
						$("a[aria-controls=pending_review_tab]").parents("[role=presentation]").css({
							display: "none"
						});
						$("a[aria-controls=audit_failure_tab]").parents("[role=presentation]").css({
							display: "block"
						});
					} else {
						$("a[aria-controls=audit_failure_tab]").parents("[role=presentation]").css({
							display: "none"
						});
						$("a[aria-controls=pending_review_tab]").parents("[role=presentation]").css({
							display: "block"
						});
					}
				})();

				if (principalStatus == 0 || principalStatus == 1) {
					manualActiveTab("perfect_info_tab");
					return;
				}
				var underReviewStatus = [10, 20, 30];
				//审核中
				if (underReviewStatus.contains(principalStatus)) {
					manualActiveTab("pending_review_tab");
					return;
				}

				var auditFailureStatus = [12, 22, 32, 99];
				//审核不通过
				if (auditFailureStatus.contains(principalStatus)) {
					manualActiveTab("audit_failure_tab");
					return;
				}

				var auditSuccStatus = [11, 21, 31, 90];
				//审核通过
				if (auditSuccStatus.contains(principalStatus)) {
					manualActiveTab("more_info_tab");
					return;
				}
			});
		}

		function submitMediaRegInfo(that, data) {
			if (data.principalId > 0) {
				var yes = confirm("修改，并提交审核？Ps：一旦提交审核，需等待审核完成才可再次操作。");
				if (!yes)
					return;
			}
			var rtype = parseInt($(that).data("rtype"));
			var url;
			if (rtype == 1) {
				url = "/daren-web/daren/submitMediaRegInfoAuditForBasic";
				if (isEmpty(data.principalName)) {
					alert("主体名称不能为空");
					return;
				}
			}
			if (rtype == 2) {
				url = "/daren-web/daren/submitMediaRegInfoAuditForContactMethod";
				if (isEmpty(data.contacts)) {
					alert("业务联系人不能为空");
					return;
				}
				if (isEmpty(data.contactPhone)) {
					alert("联系电话不能为空");
					return;
				}
			}
			if (rtype == 3) {
				url = "/daren-web/daren/submitMediaRegInfoAuditForSettMethod";

				if (isEmpty(data.paymentInvoiceType)) {
					alert("开票类型 不能为空");
					return;
				}

				var type = $("[name=payMethod]:checked").val();
				if (type == 1) {

					data.paymentSettleMethod = 1;

					if (isEmpty(data.paymentPayeeVAccount)) {
						alert("V账号 不能为空");
						return;
					}

				} else {

					if (isEmpty(data.paymentSettleMethod) || data.paymentSettleMethod == 0) {
						alert("结算方式不能为空");
						return;
					}

					if (isEmpty(data.paymentPayeeBank) || data.paymentPayeeBank == 0) {
						alert("开户银行 不能为空");
						return;
					}
					if (isEmpty(data.paymentPayeeBankBranch)) {
						alert("开户支行 不能为空");
						return;
					}
					if (isEmpty(data.paymentPayeeName)) {
						alert("收款人姓名 不能为空");
						return;
					}
					if (isEmpty(data.paymentPayeeBankAccount)) {
						alert("收款人银行卡号 不能为空");
						return;
					}
				}
			}

			$.post(url, data, function(resp) {
				if (resp.status == 0) {
					alert("操作成功");
					$("#media_reg_info_modal").modal("hide");
				} else {
					alert(resp.msg);
				}
			});
		}

