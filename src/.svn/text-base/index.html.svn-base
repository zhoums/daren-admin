	<script type="text/javascript">
		var dict = JSON.parse(getCacheDict()).realValue;
		var showTip = true;

		function manualActiveTab(target) {
			window.targetTab = target;
			var tabBtn = $("#perfect_principal_info_modal [aria-controls=" + target + "]");
			//禁用其他tab
			tabBtn.parents("[role=presentation]").siblings("[role=presentation]").find("a").on('show.bs.tab', function(e) {
				if ($(this).attr("aria-controls") != window.targetTab) {
					return false;
				}
			});
			//点击tab
			tabBtn[0].click();
		}

		function opendEditPrincipalInfoModal() {
			$("#media_reg_info_modal").modal("show");
			showTip = false;
